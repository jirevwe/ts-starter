{
  "name": "getequity-template",
  "version": "0.0.0",
  "description": "GetEquity Template",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server",
    "start:dev": "nodemon ./dist/server | bunyan -l debug",
    "start:debug": "nodemon --inspect ./dist/server | bunyan -l debug",
    "watch:tsc": "rm -rf ./dist && tsc --watch -p ./tsconfig.json",
    "build:tsc": "rm -rf ./dist && tsc -p ./tsconfig.json",
    "compile": "tsc",
    "start:worker": "node ./dist/workers/index",
    "start:worker:dev": "nodemon ./dist/workers/index | bunyan",
    "start:worker:debug": "nodemon --inspect ./dist/server/workers/index | bunyan",
    "test": "rm -f logs.json && NODE_ENV=test jest --runInBand --forceExit"
  },
  "author": "GetEquity LLC",
  "license": "ISC",
  "dependencies": {
    "@random-guys/eventbus": "^2.0.1",
    "@random-guys/gateman": "^2.0.0",
    "@random-guys/iris": "^1.1.0",
    "amqplib": "^0.5.3",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.2",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "date-fns": "^2.18.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-request-id": "^1.4.1",
    "helmet": "^3.18.0",
    "http-status-codes": "^1.3.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.2.0",
    "joi": "^17.4.2",
    "lodash": "^4.17.11",
    "module-alias": "^2.1.0",
    "mongoose": "^5.13.7",
    "nanoid": "^3.1.25",
    "nock": "^12.0.3",
    "prom-client": "^12.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "response-time": "^2.3.2",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.1",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bunyan": "^1.8.5",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.5",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.119",
    "@types/morgan": "^1.7.35",
    "@types/redis": "^2.8.10",
    "@types/supertest": "^2.0.7",
    "faker": "^4.1.0",
    "jest": "^26.6.3",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.0.0"
  },
  "_moduleAliases": {
    "@app": "./dist"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
